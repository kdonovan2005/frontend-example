<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
  </head>
  <body>

    <div class="wrapper">

      <!-- begin header image -->

      <section class="header">

        <div class="header-image">

          <img src="http://i.imgur.com/aGmuV1p.jpg" alt="" id="main-image"/>
          <!-- begin header text -->
          <h1 id="brand-header-text">Achieve a new level<br>of great looking style.</h1>
          <!-- end header text -->
          <!-- begin brand logo -->
          <img src="http://i.imgur.com/ohAD6rh.png" alt="logo" id="brand-logo"/>
          <!-- end brand logo -->
        </div>

      </section>

      <!-- end header image -->

      <!-- begin feature suite slider -->

      <section class="feature-suite">

        <div class="slideshow-container">

          <!-- begin slider images -->
          <div class="mySlides fade">
            <img src="http://i.imgur.com/7v56mIU.jpg">
          </div>

          <div class="mySlides fade">
            <img src="http://i.imgur.com/lyAdDZz.jpg">
          </div>

          <div class="mySlides fade">
            <img src="http://i.imgur.com/qSfpkRZ.jpg">
          </div>

          <!-- end slider images -->

          <!-- begin slider navigation -->

          <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
          <a class="next" onclick="plusSlides(1)">&#10095;</a>

          <div style="text-align:center">
            <span class="dot" onclick="currentSlide(1)"></span>
            <span class="dot" onclick="currentSlide(2)"></span>
            <span class="dot" onclick="currentSlide(3)"></span>
          </div>

          <!-- end slider navigation -->

        </div>

      </section>

      <!-- end feature suite slider -->

      <!-- begin nav bar -->

      <section class="nav-bar">

        <div class="menu">

          <ul>

            <!-- begin main nav items -->

            <li>Explore Products

              <!-- begin pop up nav items -->

              <ul>

                <li><a href="/pages/category">DRESSES</a></li>
                <li><a href="/pages/category">T-SHIRTS & KNIT TOPS</a></li>
                <li><a href="/pages/category">BLOUSES</a></li>
                <li><a href="/pages/category">SWEATERS</a></li>
                <li><a href="/pages/category">SKIRTS</a></li>
                <li><a href="/pages/category">PANTS</a></li>
                <li><a href="/pages/category">SCARVES</a></li>
                <li><a href="/pages/category">SHOES</a></li>
                <li><a href="/pages/category">JEWELRY</a></li>
                <li><a href="/pages/category">HANDBAGS</a></li>

              </ul>

              <!-- end pop up nav items -->

            </li>

            <li class="main-nav" id="my-list-image">

              <div class="my-list-image">

                <img src="http://i.imgur.com/Faek38M.png" alt="My List" id="my-list-image"/>
                <p id="my-list-text">My List</p>

              </div>

            </li>

            <!-- end main nav items -->

          </ul>

        </div>

      </section>

      <!-- end nav bar -->

      <!-- begin modal -->

      <div class="my-list-modal">

        <div class="my-list-modal-content-full">

          <!-- begin modal header -->

          <div class="my-list-modal-header">

            <!-- my list icon and text -->
            <img src="http://i.imgur.com/Faek38M.png" alt="" />
            <p id="my-list-modal-header-text">My List</p>

            <!-- close button -->
            <div class="close-modal-button">
              <div class="line1"></div>
              <div class="line2"></div>
            </div>

          </div>

          <!-- begin main content -->

          <div class="my-list-modal-content">

            <h2>View Saved Products</h2>

            <!-- begin mylist items -->

            <ul class="flex-container-mylist">
              <li class="flex-item-mylist">
                <img src="" alt="" />
                <p>Model Number</p>
                <h4>Model Name</h4>
              </li>
              <li class="flex-item-mylist">
                <img src="" alt="" />
                <p>Model Number</p>
                <h4>Model Name</h4>
              </li>
              <li class="flex-item-mylist">
                <img src="" alt="" />
                <p>Model Number</p>
                <h4>Model Name</h4>
              </li>
              <li class="flex-item-mylist">
                <img src="" alt="" />
                <p>Model Number</p>
                <h4>Model Name</h4>
              </li>
              <li class="flex-item-mylist">
                <img src="" alt="" />
                <p>Model Number</p>
                <h4>Model Name</h4>
              </li>
            </ul>

            <!-- end mylist items -->

          </div>

          <!-- begin slider -->

          <div class="share-mylist">

            <h2 class="keyboard-slider">Share your List</h2>

            <div class="slider-div">

              <div class="row padding-div">

                <div class="col-3">

                  <div class="padding-div">

                    <h3 id="send-and-share">Send & Share</h3>
                    <h2 id="send-mylist">My List</h2>

                    <p id="send-text">Separate with a comma to add more recipients to your send list.</p>

                  </div>

                </div>

                <div class="col-6">

                  <div class="padding-div">

                    <div class="keyboard-bg">

                      <!-- begin keyboard and text area -->

                      <div id="container">

                        <!-- text area -->
                        <textarea id="write" rows="1"></textarea>

                        <!-- begin keyboard -->

                        <div class="keyboard-area">

                          <!-- first row -->
                          <ul class="flex-container-keyboard" id="keyboard">
                            <li class="symbol"><span class="off">q</span><span class="on">1</span></li>
                            <li class="symbol"><span class="off">w</span><span class="on">2</span></li>
                            <li class="symbol"><span class="off">e</span><span class="on">3</span></li>
                            <li class="symbol"><span class="off">r</span><span class="on">4</span></li>
                            <li class="symbol"><span class="off">t</span><span class="on">5</span></li>
                            <li class="symbol"><span class="off">y</span><span class="on">6</span></li>
                            <li class="symbol"><span class="off">u</span><span class="on">7</span></li>
                            <li class="symbol"><span class="off">i</span><span class="on">8</span></li>
                            <li class="symbol"><span class="off">o</span><span class="on">9</span></li>
                            <li class="symbol"><span class="off">p</span><span class="on">0</span></li>
                            <li class="delete special-bg" id="backspace"></li>
                          </ul>

                          <!-- second row -->
                          <ul class="flex-container-keyboard" id="keyboard">
                            <li class="symbol"><span class="off">a</span><span class="on">!</span></li>
                            <li class="symbol"><span class="off">s</span><span class="on">@</span></li>
                            <li class="symbol"><span class="off">d</span><span class="on">#</span></li>
                            <li class="symbol"><span class="off">f</span><span class="on">$</span></li>
                            <li class="symbol"><span class="off">g</span><span class="on">%</span></li>
                            <li class="symbol"><span class="off">h</span><span class="on">^</span></li>
                            <li class="symbol"><span class="off">j</span><span class="on">&</span></li>
                            <li class="symbol"><span class="off">k</span><span class="on">*</span></li>
                            <li class="symbol"><span class="off">l</span><span class="on">(</span></li>
                            <li class="symbol"><span class="off">/</span><span class="on">)</span></li>
                          </ul>

                          <!-- third row -->
                          <ul class="flex-container-keyboard" id="keyboard">
                            <li class="symbol"><span class="off">*</span><span class="on">-</span></li>
                            <li class="symbol"><span class="off">z</span><span class="on">_</span></li>
                            <li class="symbol"><span class="off">x</span><span class="on">+</span></li>
                            <li class="symbol"><span class="off">c</span><span class="on">=</span></li>
                            <li class="symbol"><span class="off">v</span><span class="on">:</span></li>
                            <li class="symbol"><span class="off">b</span><span class="on">;</span></li>
                            <li class="symbol"><span class="off">n</span><span class="on">|</span></li>
                            <li class="symbol"><span class="off">m</span><span class="on">\</span></li>
                            <li class="symbol"><span class="off">*</span><span class="on">?</span></li>
                          </ul>

                          <!-- fourth row -->
                          <ul class="flex-container-keyboard" id="keyboard">
                            <li class="symbol left-shift"><span class="off">123</span><span class="on">ABC</span></li>
                            <li class="letter">.</li>
                            <li class="space">&nbsp;</li>
                            <li class="letter">@</li>
                            <li class="letter"></li>
                          </ul>

                          <br style="clear: both">

                        </div>

                        <!-- end keyboard -->

                      </div>

                      <!-- end keyboard and textarea -->

                    </div>

                  </div>

                </div>

                <div class="col-3">

                  <div class="padding-div-buttons">

                    <div class="action-buttons">

                      <div class="send-button">Send</div>
                      <div class="cancel-button">Cancel</div>

                    </div>

                  </div>

                </div>

              </div>

            </div>

          </div>

          <!-- end slider -->

          <!-- end main content -->

          <!-- begin footer -->

          <div class="my-list-modal-footer">
            <h3>Do you want to start over?</h3>
            <div class="clear-list-button">
              Clear My List
            </div>
          </div>

          <!-- end footer -->

        </div>

      </div>

      <!-- end modal -->

    </div>

  </body>
</html>
